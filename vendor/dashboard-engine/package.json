{
  "name": "@nesolagus/dashboard-engine",
  "version": "0.1.0",
  "description": "Dashboard data binding and rendering engine for Nesolagus Studio and runtime",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
